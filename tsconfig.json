{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@lowcodeunit/google-apis": [
        "dist/common"
      ],
      "@lowcodeunit/google-apis/*": [
        "dist/common/*"
      ],
      "@lowcodeunit/lcu-sln-google-apis": [
        "dist/lcu-sln-google-apis"
      ],
      "@lowcodeunit/lcu-sln-google-apis/*": [
        "dist/lcu-sln-google-apis/*"
      ],
      "@lowcodeunit/lcu-el-google-apis-timezone": [
        "dist/lcu-el-google-apis-timezone"
      ],
      "@lowcodeunit/lcu-el-google-apis-timezone/*": [
        "dist/lcu-el-google-apis-timezone/*"
      ]
    }
  }
}
